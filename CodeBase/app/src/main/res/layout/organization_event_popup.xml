<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <ImageButton
            android:id="@+id/Organizer_EventPopup_View_Event_Poster"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="View Event Poster" />

        <ImageView
            android:id="@+id/Organizer_EventPopup_QR_Code"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/Organizer_EventPopup_RegistrationPeriod"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/Organizer_EventPopup_EventRuntime"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/Organizer_EventPopup_EventLocation"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>

        <TextView
            android:id="@+id/Organizer_EventPopup_"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"



    </ListView>

</androidx.constraintlayout.widget.ConstraintLayout>